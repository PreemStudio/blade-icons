{
  "name": "basecodeoy/blade-icons",
  "description": "A package to easily use icon families with Laravel Blade.",
  "license": "MIT",
  "keywords": [
    "BaseCode Oy",
    "laravel",
    "blade-icons"
  ],
  "authors": [
    {
      "name": "Brian Faust",
      "email": "hello@basecode.sh",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/basecodeoy/blade-icons",
  "require": {
    "php": "^8.2",
    "basecodeoy/laravel-package-powerpack": "^0.0.2",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "basecodeoy/php-cs-fixer-config": "^0.0.1",
    "ergebnis/composer-normalize": "^2.2",
    "friendsofphp/php-cs-fixer": "^3.15",
    "infection/infection": "^0.27.0",
    "nunomaduro/collision": "^8.0",
    "nunomaduro/larastan": "^2.0",
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "pestphp/pest-plugin-watch": "^2.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.5",
    "spatie/laravel-ray": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "BaseCodeOy\\BladeIcons\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Icons": "BaseCodeOy\\BladeIcons\\Facades\\IconFamilyRegistry"
      },
      "providers": [
        "BaseCodeOy\\BladeIcons\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
    "analyse": "vendor/bin/phpstan analyse",
    "format": "vendor/bin/php-cs-fixer fix",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  }
}
